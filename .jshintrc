{
	// per MediaWiki style guide
	"bitwise": true,
	"camelcase": true,
	"curly": true,
	"eqeqeq": true,
	"freeze": true,
	"immed": true,
	"latedef": true,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonew": true,
	"quotmark": "single",
	"trailing": true,
	"undef": true,
	"unused": true,

	// extension-specific
	"es3": true,
	"strict": true,

	"predef": [
		"window",
		"document",
		"Geo",
		"QUnit",
		"jQuery",
		"mediaWiki",
		"mediaWikiLoadStart",
		"OO"
	]
}
