{
	"name": "ImageMetrics",
	"version": "0.1.0",
	"author": [
		"Tisza Gerg≈ë"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:ImageMetrics",
	"descriptionmsg": "imagemetrics-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"extensions": {
			"EventLogging": "*"
		}
	},
	"MessagesDirs": {
		"ImageMetrics": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"ImageMetricsHooks": "includes/ImageMetricsHooks.php"
	},
	"ResourceModules": {
		"ext.imageMetrics": {
			"scripts": [
				"logger/Logger.js",
				"logger/LoadingTimeLogger.js"
			],
			"dependencies": [
				"oojs",
				"schema.ImageMetricsLoadingTime"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.imageMetrics.head": {
			"scripts": "head.js",
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.imageMetrics.loader": {
			"scripts": "loader.js",
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "ImageMetrics/resources"
	},
	"EventLoggingSchemas": {
		"ImageMetricsLoadingTime": 10078363
	},
	"Hooks": {
		"BeforePageDisplay": "ImageMetricsHooks::onBeforePageDisplay",
		"ResourceLoaderGetConfigVars": "ImageMetricsHooks::onResourceLoaderGetConfigVars",
		"ResourceLoaderTestModules": "ImageMetricsHooks::onResourceLoaderTestModules"
	},
	"config": {
		"ImageMetricsSamplingFactor": false,
		"ImageMetricsLoggedinSamplingFactor": false
	},
	"manifest_version": 1
}
